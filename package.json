    "babel-eslint": "^10.1.0",
    "conventional-changelog-cli": "^2.1.1",
    "eslint": "^7.3.1",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-vue": "^6.2.2",
    "frontmatter-markdown-loader": "^3.6.0",
    "husky": "4.2.5",
    "lint-staged": "^11.2.0",
    "markdown-it": "^12.3.2",
    "vuepress": "^1.5.2",
    "vuepress-plugin-demo-code": "^0.5.0",
